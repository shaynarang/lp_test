<h1>Your Parsed File</h1>

<div class="parsed_file">
  <% @upload.sections.each do |section| %>
    <div id="section">
        <p class="section_title"> [<%= section.title %>] </p>
        <% section.keys.each do |key|%>
          <p> <%= key.title %>: <%= key.values.first.content %> </p>
        <% end %>
    </div>
  <% end %>
</div>

<p id="upload_link"> <%= link_to "Edit", edit_upload_path(@upload) %> </p>
<p id="delete_link"> <%= link_to "Delete", @upload, :method => "delete", data: { confirm: 'Are you sure?' }%>
<p id="index_link"> <%= link_to "View All Uploads", uploads_path %> </p>
<p id="root_link"> <%= link_to "Upload Another File", root_path %> </p>